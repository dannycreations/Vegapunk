{
	"name": "@vegapunk/utilities",
	"version": "1.0.57",
	"author": "dannycreations",
	"license": "GPL-3.0",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./common": {
			"types": "./dist/lib/std/common.d.ts",
			"default": "./dist/lib/std/common.js"
		},
		"./result": {
			"types": "./dist/lib/std/result.d.ts",
			"default": "./dist/lib/std/result.js"
		},
		"./sleep": {
			"types": "./dist/lib/std/sleep.d.ts",
			"default": "./dist/lib/std/sleep.js"
		},
		"./time": {
			"types": "./dist/lib/std/time.d.ts",
			"default": "./dist/lib/std/time.js"
		}
